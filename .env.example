# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# AI Providers
ANTHROPIC_API_KEY=your_claude_api_key
# Google Gemini API key - Get yours from https://makersuite.google.com/app/apikey
GOOGLE_GEMINI_API_KEY=your_gemini_api_key
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_for_client_side
# Alternative naming for Google AI API key (used by meal planner)
GOOGLE_AI_API_KEY=your_gemini_api_key
NEXT_PUBLIC_GOOGLE_AI_API_KEY=your_gemini_api_key_for_client_side

# Security
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# OAuth Providers (Optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=kecarajocomer

# Analytics Configuration (PostHog)
# Get your PostHog key from https://app.posthog.com/project/settings
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_project_api_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Debug Mode (Development only)
NEXT_PUBLIC_DEBUG_AUTH=false
NEXT_PUBLIC_DEBUG_AI=false

# Error Tracking (Sentry)
# Get your DSN from https://sentry.io - Create a Next.js project
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# Product Analytics (Mixpanel)
# Get your token from https://mixpanel.com - Project Settings
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_project_token

# MercadoPago (Payments)
# Get credentials from https://www.mercadopago.com.ar/developers/panel
MERCADOPAGO_ACCESS_TOKEN=your_mercadopago_access_token
MERCADOPAGO_PUBLIC_KEY=your_mercadopago_public_key